<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17.0.2-ea" fx:controller="is.LesaSkrifaController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" />
    <Button onAction="#onVistaButtonClick" text="Vista" />
    <Button onAction="#onLesaButtonClick" text="Lesa" />
   <GridPane gridLinesVisible="true">
     <columnConstraints>
       <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
       <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
     </columnConstraints>
     <rowConstraints>
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
     </rowConstraints>
      <children>
         <Label fx:id="fxType" prefHeight="40.0" prefWidth="100.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         <Label layoutX="110.0" layoutY="40.0" prefHeight="40.0" prefWidth="100.0" text="type" GridPane.rowIndex="1" />
         <Label layoutX="10.0" layoutY="40.0" prefHeight="40.0" prefWidth="100.0" text="color" GridPane.rowIndex="2" />
         <Label fx:id="fxColor" layoutX="10.0" layoutY="40.0" prefHeight="40.0" prefWidth="100.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      </children>
   </GridPane>
</VBox>
